{
  "name": "ListaFamiliar",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "productos": {
      "type": "hasMany",
      "model": "Producto",
      "foreignKey": "listaFamiliarId"
    },
    "propietario": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "owner"
    },
    "miembros": {
      "type": "hasMany",
      "model": "Usuario",
      "foreignKey": "listaFamiliarId"
    },
    "solicitudes": {
      "type": "hasAndBelongsToMany",
      "model": "Usuario",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
